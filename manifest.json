{
    "name": "Sitemagic CMS",
    "id": "sitemagiccms",
    "packaging_format": 1,
    "description": {
        "en": "Content Management System",
        "fr": "SystÃ¨me de gestion de contenu"
    },
    "version": "4.4.2~ynh1",
    "url": "https://sitemagic.org/",
    "upstream": {
        "license": "free",
        "website": "https://sitemagic.org/",
        "admindoc": "https://sitemagic.org/sites/cms-guide/",
        "code": "https://github.com/Jemt/SitemagicCMS"
    },
    "license": "free",
    "maintainer": {
        "name": "eric_G",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.2.4"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/sitemagic",
                "default": "/sitemagic"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "language",
                "type": "string",
                "ask": {
                    "en": "Choose the application language",
                    "fr": "Choisissez la langue de l'application"
                },
                "choices": ["da", "de", "el", "fr", "en", "sr"],
                "default": "fr"
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
